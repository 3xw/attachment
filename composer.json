{
  "name": "3xw/attachment",
  "description": "Attachment plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.2",
    "cakephp/cakephp": "4.x-dev",
    "friendsofcake/crud": "dev-bug-paging",
    "friendsofcake/search": "cake-4.x-dev",
    "3xw/cakephp-utils":"next-dev",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-ziparchive": "^1.0",
    "intervention/image": "^2.3",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/3xw/crud"
    }
  ],
  "autoload": {
    "psr-4": {
      "Attachment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Attachment\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "minimum-stability": "dev"
}
