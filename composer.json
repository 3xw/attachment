{
    "name": "3xw/attachment",
    "description": "Attachment plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.3",
        "friendsofcake/crud": "^5.0",
        "friendsofcake/search": "^4.0",
        "wyrihaximus/fly-pie": "^1.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "intervention/image": "^2.3",
        "cakephp/migrations": "@stable",
        "firebase/php-jwt": "^5.0",
        "3xw/cakephp-utils":"dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Attachment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Attachment\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
